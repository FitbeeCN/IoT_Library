    "gulp": "^3.9.0",
    "gulp-jsdoc": "^0.1.4",
    "gulp-mocha": "^2.1.3",
    "gulp-watch": "^4.2.4",
    "mocha": "*",
    "plato": "^1.5.0"
  },
  "browser": {
    "fs": "localstorage-fs",
    "telehash-udp4": false,
    "telehash-tcp4": false,
    "./test/thtp.test.js": "./test/thtp.browser.test.js"
  },
  "bin": {
    "router": "./bin/router.js",
    "mesh": "./bin/mesh.js"
  },
  "scripts": {
    "browserify": "browserify index.js -o bundle.js",
    "browser": "browserify index.js -o bundle.js && node test/browser/server.js",
