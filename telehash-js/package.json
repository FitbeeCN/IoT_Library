    "localstorage-fs": "~0.1.0",
    "optimist": "~0.6.1",
    "path-extra": "~0.3.0",
    "telehash-http": "^0.1.0",
    "telehash-tcp4": "^0.0.8",
    "telehash-udp4": "^0.0.14",
    "telehash-webrtc": "0.0.8"
  },
  "devDependencies": {
    "chai": "*",
    "chext": "0.0.1",
    "concat-stream": "~1.4.6",
